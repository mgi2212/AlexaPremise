	if (this.FriendlyName = "") then
		this.discoveryJson = ""
		exit sub
	end if
	tmp = "{"
	tmp = tmp + JsonNameValue("endpointID", mid(this.ObjectID,2,36)) + ","
	tmp = tmp + JsonNameValue("friendlyName",this.FriendlyName) + ","
	tmp = tmp + JsonNameValue("description",this.FriendlyDescription) + ","
	tmp = tmp + JsonNameValue("manufacturerName", "Premise Server") + ","
	tmp = tmp + JsonNameArray("displayCategories", this.displayCategories) + ","
	tmp = tmp + chr(34) + "cookie" + chr(34) + ":" + "{ " + JsonNameValue("path", this.Path) + " },"
	tmp = tmp + JsonNameArray("capabilities", this.renderDiscoveryJson())
	tmp = tmp + "}"
	this.discoveryJson = tmp
end if
tmp = ""
