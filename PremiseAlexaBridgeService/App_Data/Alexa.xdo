            <Object ID="{3A3773B8-B3C9-498A-A654-D86A9F3E2F65}" Name="renderDiscoveryJson" Class="sys://Schema/Scripting/ClassFunction" >
              <![CDATA[
Script=
json = ""
set classes = this.GetClasses()
for each cls in classes
	if (cls.Name = "Alexa.PowerController") then
		if (json <> "") then
			json = json + ","
		end if
		json = json + "{"
		json = json + this.discoveryJsonProlog()
		json = json + JsonNameValue("interface",cls.Name) + ","
		json = json + chr(34) + "properties" + chr(34) + ": {" + chr(34) + "supported" + chr(34) + ": [ {"
		json = json + JsonNameValue("name","powerState") + "} ],"
		json = json + chr(34) + "proactivelyReported" + chr(34) + ":" + LCase(CStr(this.proactivelyReported)) + ","
		json = json + chr(34) + "supportsDeactivation" + chr(34) + ":" + LCase(CStr(this.supportsDeactivation)) + ","
		'json = json + this.discoveryJsonEpilog()
		json = json + "}"
		json = json + "}"
	end if
next

method.renderDiscoveryJson = json
json = ""
]]>
